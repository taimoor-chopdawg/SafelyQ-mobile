{
  "name": "safelyq",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "port-8088": "npx react-native start --port=8088",
    "ios": "npx react-native run-ios",
    "small": "react-native run-ios --simulator='iPhone 8'",
    "pod-install": "cd ios && pod install && cd ..",
    "reset-cache": "npm start -- --reset-cache",
    "build-clean": "cd android && ./gradlew clean && cd ..",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "cd android && ./gradlew bundleRelease && cd ..",
    "test-release": "cd android && ./gradlew assembleRelease && cd ..",
    "remove-app": "adb uninstall com.safelyq",
    "test-release-app": "cd android && ./gradlew app:assembleRelease && cd ..",
    "android-permission-execute": "chmod 755 android/gradlew",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "peer-legacy": "npm install --legacy-peer-deps",
    "peer-legacy-permanent": "npm config set legacy-peer-deps true",
    "peer-legacy-save": "npm install --save --legacy-peer-deps",
    "open-xcode": "xed -b ios"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@microsoft/signalr": "^6.0.10",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/cameraroll": "^4.1.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^9.3.5",
    "@react-native-firebase/analytics": "14.11.1",
    "@react-native-firebase/app": "14.11.1",
    "@react-native-firebase/crashlytics": "14.11.1",
    "@react-native-firebase/firestore": "14.11.1",
    "@react-native-firebase/functions": "14.11.1",
    "@react-native-firebase/messaging": "14.11.1",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "axios": "^1.1.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.38",
    "react": "18.1.0",
    "react-native": "0.70.1",
    "react-native-app-auth": "^6.4.3",
    "react-native-appstore-version-checker": "^3.0.0",
    "react-native-blob-util": "^0.16.3",
    "react-native-braintree": "^0.5.0",
    "react-native-braintree-dropin-ui": "^1.1.3",
    "react-native-calendar-strip": "^2.2.6",
    "react-native-communications": "^2.2.1",
    "react-native-config": "^1.4.11",
    "react-native-document-picker": "^8.1.2",
    "react-native-dropdown-picker": "^5.4.2",
    "react-native-gesture-handler": "^2.6.2",
    "react-native-gifted-chat": "^1.0.4",
    "react-native-image-gallery": "^2.1.5",
    "react-native-image-picker": "^4.10.0",
    "react-native-lightbox": "^0.8.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-material-menu": "^2.0.0",
    "react-native-onesignal": "^4.4.1",
    "react-native-permissions": "^3.6.1",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
